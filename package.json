{
  "name": "@zeus-lint/eslint-config",
  "version": "1.0.1-beta.1",
  "packageManager": "pnpm@8.10.5",
  "description": "En11x's eslint config",
  "main": "./dist/index.js",
  "type": "module",
  "homepage": "https://github.com/En11x/eslint-config",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "dev": "tsup  --format esm,cjs --watch & eslint-flat-config-viewer",
    "lint": "tsup --format esm && eslint .",
    "lint:fix": "tsup --format esm && eslint --fix .",
    "test": "vitest",
    "release": "bumpp && pnpm publish"
  },
  "keywords": [
    "eslint-config"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "@En11x",
  "license": "ISC",
  "devDependencies": {
    "@types/eslint": "^8.44.7",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.9.1",
    "bumpp": "^9.2.0",
    "eslint": "^8.53.0",
    "eslint-define-config": "^1.24.1",
    "eslint-flat-config-viewer": "^0.1.1",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.1.1",
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@stylistic/eslint-plugin": "^1.3.1",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint-config-flat-gitignore": "^0.1.1",
    "eslint-plugin-i": "^2.29.0",
    "eslint-plugin-perfectionist": "^2.4.0",
    "local-pkg": "^0.5.0"
  }
}
